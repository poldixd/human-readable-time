{
    "name": "poldixd/human-readable-time",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Poltmann",
            "email": "dev@nils.li"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/pest-plugin-snapshots": "^2.0",
        "spatie/pest-plugin-test-time": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "poldixd\\HumanReadableTime\\": "src/poldixd/HumanReadableTime/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "poldixd\\HumanReadableTime\\Providers\\HumanReadableTimeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
