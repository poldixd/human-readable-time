{
    "name": "poldixd/human-readable-time",
    "license": "MIT",
    "authors": [
        {
            "name": "Nils Poltmann",
            "email": "dev@nils.li"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "poldixd\\HumanReadableTime\\": "src/poldixd/HumanReadableTime/"
